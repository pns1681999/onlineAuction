<div class="counterup-area">
    <div class="container-fluid p-0">
        <div class="row no-gutters">

            <div class="col-lg-4 col-md-6">
                <!-- Begin limupa Counter Area -->
                <div class="limupa-counter gray-bg">
                    <div class="counter-img">
                        <i class="fa fa-thumbs-up"></i>
                    </div>
                    <div class="counter-info">
                        <div class="counter-number">
                            <h3 class="counter">{{User.DiemCong}}</h3>
                        </div>
                        <div class="counter-text">
                            <span>LIKES</span>
                        </div>
                    </div>
                </div>
                <!-- limupa Counter Area End Here -->
            </div>
            <div class="col-lg-4 col-md-6">
                <!-- Begin limupa Counter Area -->
                <div class="limupa-counter gray-bg">
                    <div class="counter-img">
                    <i class="fa fa-thumbs-down"></i>
                    </div>
                    <div class="counter-info">
                            <h3 class="counter">{{User.DiemTru}}</h3>
                        <div class="counter-text">
                            <span>DISLIKES</span>
                        </div>
                    </div>
                </div>
                <!-- limupa Counter Area End Here -->
            </div>
            <div class="col-lg-4 col-md-4">
                <!-- Begin limupa Counter Area -->
                <div class="limupa-counter gray-bg">
                    <div class="counter-img">
                    <i class="fa fa-check"></i>
                    </div>
                    <div class="counter-info">
                            <div class="counter-text">
                            <span>COIN</span>
                        </div>
                            <h3 class="counter">{{percent}}</h3>
                        
                    </div>
                </div>
                <!-- limupa Counter Area End Here -->
            </div>
         </div>
    </div>
</div>