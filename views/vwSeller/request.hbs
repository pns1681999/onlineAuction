{{#if empty}}

<div class="error404-area pt-30 pb-60">
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <div class="error-wrapper text-center ptb-50 pt-xs-20">
                    <div class="error-text">
                        <h1>Empty</h1>
                    </div>
                    <div class="error-button">
                        <a href="/">Back to home page</a>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

{{else}}

<div class="frequently-area pt-60 pb-50">
    <div class="container">
        <div class="row">
            <table class="table table-striped">
                <thead>
                    <tr>
                        <th scope="col">Bidder</th>
                        <th scope="col">Sản phẩm</th>
                        <th scope="col">Cho phép</th>
                        <th scope="col">Không cho phép</th>
                    </tr>
                </thead>
                <tbody>
                    {{#each this.listReq}}
                    <form action="/seller/agree/bidder={{IdNguoiMua}}/product={{IdSanPham}}"
                        id="frmAgree{{IdNguoiMua}}-{{IdSanPham}}" method="POST">
                    </form>
                    <form action="/seller/disagree/bidder={{IdNguoiMua}}/product={{IdSanPham}}"
                        id="frmDisagree{{IdNguoiMua}}-{{IdSanPham}}" method="POST">
                    </form>
                    <tr>
                        <td>{{Bidder.HoVaTen}}</td>
                        <td>{{SanPham.TenSanPham}}</td>
                        <td><a href="javascript: $('#frmAgree{{IdNguoiMua}}-{{IdSanPham}}').submit();"><i class="fa fa-check text-success"</i></a></td>
                        <td><a href="javascript: $('#frmDisagree{{IdNguoiMua}}-{{IdSanPham}}').submit();"><i class="fa fa-remove text-danger"></i></td>
                    </tr>
                    {{/each}}

                </tbody>
            </table>
        </div>
    </div>
</div>
{{/if}}