<div class="card-body">
  <form method="POST">
  <div class="form-group">
      <label for="txtCatName">Tên danh mục</label>
      <input type="text" class="form-control" id="txtCatName" name="txtCatName"
      autofocus>
  </div>
  <div class="form-group">
    <label for="txtCatType">Thuộc danh mục</label>
    <select id="txtCatType" name="txtCatType">
      <option value="0">None</option>
      {{#each this.danhmuc}} 
        <option value="{{IdDanhMuc}}">{{TenDanhMuc}}</option>
      {{/each}}
    </select>
  </div>
  <button type="submit" class="btn btn-outline-success">
      <i class="fa fa-check" aria-hidden="true"></i>
      Save
  </button>
  </form>
</div>