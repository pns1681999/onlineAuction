                            <label>Tình trạng</label>
                            <a class="form-control">{{this.product.TinhTrang}}</a>
                        </div>
                        <div class="form-group">
                            <label>Đánh giá</label>
                            <a class="form-control">{{this.product.DanhGia}}</a>
