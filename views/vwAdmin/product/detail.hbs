                            <a class="form-control">{{this.product.IdSanPham}}</a>
                        </div>
                        <div class="form-group">
                            <label>Tên sản phẩm</label>
                            <a class="form-control">{{this.product.TenSanPham}}</a>
                        </div>
                        <div class="form-group">
                            <label>Loại sản phẩm</label>
                            <a class="form-control">{{this.LoaiSanPham.TenDanhMuc}}</a>
